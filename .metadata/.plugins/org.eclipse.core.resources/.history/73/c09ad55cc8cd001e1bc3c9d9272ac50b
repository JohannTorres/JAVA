package tech.johann.beeJava;

import java.util.Scanner;

public class Desafio1035 {

	public static void main(String[] args) {
		/**
		 * Leia 4 valores inteiros A, B, C e D. A seguir, se B for maior do que C 
		 * e se D for maior do que A, e a soma de C com D for maior que a soma de 
		 * A e B e se C e D, ambos, forem positivos e se a variável A for par 
		 * escrever a mensagem "Valores aceitos", senão escrever "Valores nao aceitos".
		 **/
		
		Scanner input = new Scanner(System.in);
		
		int a = input.nextInt();
		int b = input.nextInt();
		int c = input.nextInt();
		int d = input.nextInt();
		
		boolean cond1 = b > c;
		boolean cond2 = d> a;
		boolean cond3 = (c+d) > (a+b);
		boolean cond4 = c > 0;
		boolean cond5 = d > 0;
		boolean cond6 = a%2 == 0;
		
		if (b > c && d > a && (c+d) > (a+b) && c > 0 && c > 0 && d > 0 && a%2 == 0) {
			System.out.println("Valores Aceitos");
		}
		else {
			System.out.println("Valores Não Aceitos");
		}
		
		static void {
			boolean cond1, cond2, cond3, cond, cond5, cond6;
			
			
		}
		
		
		
		
		
		
		
		
		 
		
		
	}

}
