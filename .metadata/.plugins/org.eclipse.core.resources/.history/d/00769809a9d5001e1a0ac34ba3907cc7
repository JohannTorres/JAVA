package tech.johann.nelioUdemy;

public class Aula32_If_Else {

	public static void main(String[] args) {
		
		double x = 11.99;
		
		if (x >= 0 && x < 12) {
		System.out.println("BOM DIA!");
		}
		if (x >= 12 && x < 18) {
		System.out.println("BOA TARDE!");
		}
		if (x>= 18 && x < 24) {
		System.out.println("BOA NOITE!");
		}
		

	}

}
