package tech.johann.beeJava;

import java.util.Scanner;

public class Desafio1035_v2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		Scanner input = new Scanner(System.in);
		
		int a = input.nextInt();
		int b = input.nextInt();
		int c = input.nextInt();
		int d = input.nextInt();
		
		boolean cond1 = b > c;
		boolean cond2 = d> a;
		boolean cond3 = (c+d) > (a+b);
		boolean cond4 = c > 0;
		boolean cond5 = d > 0;
		boolean cond6 = a%2 == 0;
		
		

	}

}
