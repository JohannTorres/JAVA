package tech.johann.ContaSystemHeranca;

public class Conta {

	public static void main(String[] args) {
		
		int numero;
		double saldo;
		int quantidade;

		void Sacar(double quantidade) {
			double novoSaldo  = this.saldo - quantidade;
			this.saldo = novoSaldo;

		void Depositar() {
			double novoSaldo  = this.saldo + quantidade;
			this.saldo = novoSaldo;
		void getSaldo() {
			double novoSaldo  = this.saldo - quantidade;
			this.saldo = novoSaldo;
		
		

	}

}
