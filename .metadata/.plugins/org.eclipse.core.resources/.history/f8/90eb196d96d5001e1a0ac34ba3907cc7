package tech.johann.nelioUdemy;

import java.util.Scanner;

public class Aula29_Exercicio3 {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		int A, B, C, D;
		double diferenca;
		
		A = sc.nextInt();
		B = sc.nextInt();
		C = sc.nextInt();
		D = sc.nextInt();
		
		diferenca = (A*B-C*D);
		
		System.out.println("DIFERENÃ‡A: "+diferenca);
		
		

	}

}
